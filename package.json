{
  "name": "viiksetjs",
  "version": "0.0.29",
  "description": "Javascript graphing utility based on SVG",
  "homepage": "https://jamestthompson3.github.io/viiksetjs",
  "main": "lib/index.js",
  "repository": "https://github.com/jamestthompson3/viiksetjs.git",
  "author": "Taylor Thompson <taylor@easyanticheat.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf ./lib && babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "lint": "eslint --fix --ext .js ./src/",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix ./src/",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@vx/axis": "^0.0.183",
    "@vx/bounds": "^0.0.182",
    "@vx/curve": "^0.0.182",
    "@vx/gradient": "^0.0.183",
    "@vx/grid": "^0.0.183",
    "@vx/group": "^0.0.183",
    "@vx/pattern": "^0.0.183",
    "@vx/point": "^0.0.182",
    "@vx/shape": "^0.0.183",
    "@vx/threshold": "^0.0.183",
    "d3-array": "^2.0.3",
    "d3-scale": "^2.0.0",
    "d3-shape": "^1.2.0",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.11",
    "polished": "^2.0.3",
    "prop-types": "^15.6.2",
    "react-sizeme": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.91.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.13.4",
    "react": "^16.7.0",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "styled-components": "^3.4"
  }
}
